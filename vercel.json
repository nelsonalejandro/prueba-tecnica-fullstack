{
    "version": 2,
    "buildCommand": "./vercel-build.sh",
    "outputDirectory": "build",
    "builds": [
        {
            "src": "backend/src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/src/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}